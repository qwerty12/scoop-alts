{
    "version": "5.70",
    "description": "Extracts hardcoded text (hardsub) from video",
    "homepage": "https://sourceforge.net/projects/videosubfinder/",
    "license": "Public Domain",
    "suggest": {
        "vcredist": [
            "extras/vcredist2019",
            "extras/vcredist-aio"
        ]
    },
    "architecture": {
        "64bit": {
            "url": "https://downloads.sourceforge.net/project/videosubfinder/VideoSubFinder_5.70_x64.zip",
            "hash": "sha1:c9dfdf3649cba4f32adde7b21de5150644190e1d",
            "extract_dir": "Release_x64"
        },
        "32bit": {
            "url": "https://downloads.sourceforge.net/project/videosubfinder/VideoSubFinder_5.70_x86.zip",
            "hash": "sha1:0640359a05ed98302743304c6efd176f0a017702",
            "extract_dir": "Release"
        }
    },
    "persist": [
        "settings\\general.cfg",
        "TXTImages",
        "TXTImagesJoined",
        "TXTResults"
    ],
    "bin": "VideoSubFinderWXW.exe",
    "shortcuts": [
        [
            "VideoSubFinderWXW.exe",
            "VideoSubFinder"
        ]
    ],
    "checkver": {
        "sourceforge": "videosubfinder/",
        "regex": "VideoSubFinder_([\\d.+]+)_x64.zip"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://downloads.sourceforge.net/project/videosubfinder/VideoSubFinder_$version_x64.zip"
            },
            "32bit": {
                "url": "https://downloads.sourceforge.net/project/videosubfinder/VideoSubFinder_$version_x86.zip"
            }
        }
    }
}
